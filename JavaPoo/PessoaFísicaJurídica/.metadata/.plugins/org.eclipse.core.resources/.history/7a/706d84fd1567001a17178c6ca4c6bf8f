import javax.swing.JOptionPane;
public class PessoaFisica extends Main{
	private String cpf, rg;
	
	public void cpf(String _cpf) {
		this.cpf = _cpf;
	}
	
	public void rg(String _rg) {
		this.rg = _rg;
	}
	
	public void RPessoaFisica() {
		JOptionPane.showMessageDialog(null, "CPF: "+cpf+"\nRG: "+rg);
	}
}
