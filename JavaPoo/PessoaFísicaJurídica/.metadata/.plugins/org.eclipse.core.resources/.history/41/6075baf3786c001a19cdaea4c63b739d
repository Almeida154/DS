import javax.swing.JOptionPane;
public class PessoaJuridica extends Main{
	private String cnpj;
	
	public void setCnpj(String _cnpj) {
		this.cnpj = _cnpj;
	}
	
	public void RPessoaJuridica() {
		JOptionPane.showMessageDialog(null, "CNPJ: "+cnpj+"\nNome: "+getNome()+"\nData Nascimento ou criação: "+getDataCriacao()+"\nRua: "+getRua()+"\nNúmero: "
		+getNum()+"\nTelefone: "+getTelefone());
	}
}
