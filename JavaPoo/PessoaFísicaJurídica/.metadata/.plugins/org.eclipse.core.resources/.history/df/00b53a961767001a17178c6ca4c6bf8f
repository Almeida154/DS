import javax.swing.JOptionPane;
public class PessoaFisica extends Main{
	private String cpf, rg, name;
	
	public void setCpf(String _cpf) {
		this.cpf = _cpf;
	}
	
	public void setRg(String _rg) {
		this.rg = _rg;
	}
	
	public void RPessoaFisica() {
		name = getNome();
		JOptionPane.showMessageDialog(null, "CPF: "+cpf+"\nRG: "+rg+"\nNome: "+name+"\nData Nascimento"
				+ "ou criação: "+dataCriacao+"\nRua: "+rua+"\nNúmero: "+num+"\nTelefone: "+tel);
	}
}
